<!-- IMAGE CUSTOMIZER -->
<script>
   
    $(document).ready(function(){
   
    var svgelem ='';
    
    var prod_desc =  $('.ProductDescriptionContainer').html();
    if(prod_desc.indexOf('svg') > -1){
   
       var svgel = prod_desc.split('|');
   
       $('.ProductDescriptionContainer').html(svgel[0]);
   
       svgelem = svgel[1].replace(/&lt;/g,'<').replace(/&gt;/g,'>').replace(/<br>/g,'');
    
       $('.ProductThumbImage a').html(svgelem);
      
         
   }
    
    
    
    $('.swatchColours span').on('click', function(){
    // alert($(this).css('background-color'));
      var area = $(this).parents('.productAttributeRow').find('.name').html().split(' ');
      area = area[0].toLowerCase().trim();
   
    // $('svg').css('background-blend-mode','color');
    // $('svg rect').attr('width',$('svg image').attr('width')); $('svg rect').attr('height',$('svg image').attr('height'));
      $('svg .'+area).css('opacity',0.7);
      $('svg .'+area).css('background-blend-mode','screen');
      $('svg .'+area).css("fill",$(this).css('background-color'));
    
    })
    
  
    
     $('#ProductDetails').show();
  
})
        
</script>

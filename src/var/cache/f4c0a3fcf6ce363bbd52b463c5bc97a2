            var img = new Image();
            img.onload = good; 
            img.onerror = bad;
            img. src = src;
        }
        
        
        // attach to clicks on the radio button for swatch color, using the validation class
        $('input:radio.validation').click(function () {
            // Check how many divs we have.  We need the same # of checked radio buttons to proceed
            prodDivs = $(".productAttributeConfigurablePickListSwatch").length;
            checkedRadios = $("input[type='radio']:checked", $(".productAttributeConfigurablePickListSwatch")).length;

            //if (prodDivs === checkedRadios) {
                $(".thumb_loader_div").fadeIn();
                // Create an array to hold the colors, one to three of them
                //var allColors = new Array(0);
                

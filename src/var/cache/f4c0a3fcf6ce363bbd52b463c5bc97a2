                    newImageURL = newImageURL +  '&c' + i.toString() + '=' + allColors[i];
                }*/
                // Code added by supertron start
                
                //alert(newImageURL);
                
                var newLargeImageURL = newImageURL.replace("&width=560", "&width=768");
                var elem = document.createElement("img");
                elem.setAttribute("src", newLargeImageURL);
                document.getElementById("large_img_div").appendChild(elem);
                
                

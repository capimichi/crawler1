        <!-- Color Swatches Code Start -->
        <script type="text/javascript">
		$(document).ready(function() {
			function getContrastYIQ(hexcolor){
				var r = parseInt(hexcolor.substr(0,2),16);
				var g = parseInt(hexcolor.substr(2,2),16);
				var b = parseInt(hexcolor.substr(4,2),16);
				var yiq = ((r*299)+(g*587)+(b*114))/1000;
				return (yiq >= 128) ? '000' : 'fff';
			}


			$(".productOptionPickListSwatch li.swatch").each(function(){
				
				var hoverTimeout;
				$(this).find(".showPreview").hover(function() {
					var swatch_title = $(this).attr("title");
					posi = $(this).offset();
					var inner_style = $(this).find("span").attr('style')
					var hex_code = inner_style.split("#");

					$(".cus_img_swatch_inner").attr('style', inner_style)
					$(".cus_img_swatch_inner").css("color", "#"+getContrastYIQ(hex_code[1]));
					$("#image_swatch_preview").css("left", posi.left+30 );
					$("#image_swatch_preview").css("top", posi.top+30 );
					
					
					$(".cus_img_swatch_inner").text(swatch_title);
					
						
					hoverTimeout = setTimeout(function() {
						$("#image_swatch_preview").css("display", "block");	
					}, 50);
				}, function() {
					clearTimeout(hoverTimeout);
					$("#image_swatch_preview").css("display", "none");
				});
			});
		});
		</script>
		<div id="image_swatch_preview" class="cus_img_swatch">
        	<div class="cus_img_swatch_inner"></div>
        </div>
        <!-- Color Swatches Code End -->
        
        
        
        

